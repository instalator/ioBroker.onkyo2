  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "",
      "type": "meta",
      "common": {
        "name": "Onkyo",
        "type": "meta.user"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "command",
      "type": "state",
      "common": {
        "name": "Send raw or high level command",
        "type": "string",
        "role": "media.command",
        "read": true,
        "write": true,
        "def": ""
      },
      "native": {}
    },
    {
      "_id": "dock.prev",
      "type": "state",
      "common": {
        "name": "Controlling playback previous",
        "type": "boolean",
        "role": "button.prev",
        "read": false,
        "write": true,
        "def": false
      },
      "native": {
        "val": "TRDN"
      }
    },
    {
      "_id": "dock.next",
      "type": "state",
      "common": {
        "name": "Controlling playback next",
        "type": "boolean",
        "role": "button.next",
        "read": false,
        "write": true,
        "def": false
      },
      "native": {
        "val": "TRUP"
      }
    },
    {
      "_id": "dock.stop",
      "type": "state",
      "common": {
        "name": "Controlling playback stop",
        "type": "boolean",
        "role": "button.stop",
        "read": false,
        "write": true,
        "def": false
      },
      "native": {
        "val": "STOP"
      }
    },
    {
      "_id": "dock.pause",
      "type": "state",
      "common": {
        "name": "Controlling playback pause",
        "type": "boolean",
        "role": "button.pause",
        "read": false,
        "write": true,
        "def": false
      },
      "native": {
        "val": "PAUSE"
      }
    },
    {
      "_id": "dock.play",
      "type": "state",
      "common": {
        "name": "Controlling playback play",
        "type": "boolean",
        "role": "button.play",
        "read": false,
        "write": true,
        "def": false
      },
      "native": {
        "val": "PLAY"
      }
    },
    {
      "_id": "dock.net-usb-listinfo-select",
      "type": "state",
      "common": {
        "name": "Select number Item from listinfo (0-9)",
        "type": "number",
        "min": 0,
        "max": 9,
        "role": "state",
        "read": true,
        "write": true
      },
      "native": {}
    }
  ]
